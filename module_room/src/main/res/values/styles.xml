<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SettingOpenText">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/common_white</item>
        <item name="android:background">@drawable/room_setting_background</item>
    </style>

    <style name="RoomCommonFullButton">
        <item name="android:layout_height">56dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/common_white</item>
        <item name="android:background">@drawable/common_btn</item>
    </style>

    <style name="RoomCommonNegativeButton">
        <item name="android:layout_height">56dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/color_6C74FB</item>
        <item name="android:background">@drawable/common_negative</item>
    </style>
</resources>